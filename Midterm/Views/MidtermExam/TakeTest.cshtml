@using Midterm
@model List<TestQuestionModel>
    @Html.ValidationSummary()
<form method="post" asp-action="TakeTest" asp-controller="MidtermExam">
@for (int i = 0; i < Model.Count; i++)
{
    if (Model[i] is TrueFalseQuestionModel)
    {
        <partial name="_TrueFalseQuestion" for="@Model[i]"></partial>
    }
    if (Model[i] is MultipleChoiceQuestionModel)
    {
        <partial name="_MultipleChoiceQuestion" for="@Model[i]"></partial>
    }
    if (Model[i] is LongAnswerQuestionModel)
    {
        <partial name="_LongAnswerQuestion" for="@Model[i]"></partial>
    }
    if (Model[i] is ShortAnswerQuestionModel)
    {
        <partial name="_ShortAnswerQuestion" for="@Model[i]"></partial>
    }
}
<div>
<button type="submit" class="btn btn-primary">Submit</button>
</div>
</form>